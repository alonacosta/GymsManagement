@{
    ViewData["Title"] = "Error404";
}

<div class="shadow-none p-3 bg-body-tertiary rounded bg-white">
    <div class="container-error404">
        <h1 class="h1-error404">404 Error </h1>
        <p class="zoom-area"><b>Sorry</b>, the page not found! </p>
        <section class="error-container">
            <span class="four"><span class="screen-reader-text">4</span></span>
            <span class="zero"><span class="screen-reader-text">0</span></span>
            <span class="four"><span class="screen-reader-text">4</span></span>
        </section>
        <div class="link-container">
            <a href="javascript:history.back()" class="more-link">Back</a>
        </div>
    </div>
</div>